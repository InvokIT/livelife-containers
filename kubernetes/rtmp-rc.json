{
  "apiVersion": "v1",
  "kind": "ReplicationController",
  "metadata": {
    "name": "rtmp",
    "labels": {
      
    },
    "namespace": ""
  },
  "spec": {
    "replicas": 3,
    "selector": {
      "app": "rtmp"
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "rtmp"
        }
      },
      "spec": {
        "volumes": null,
        "containers": [
          {
            "name": "rtmp",
            "image": "eu.gcr.io/steady-cat-112112/rtmp:latest",
            "ports": [
              {
                "containerPort": 1935
              }
            ],
            "limits": {
              "memory": "200M",
              "cpu": "100m"
            }
          },
          {
            "name": "rtmp-controller",
            "image": "eu.gcr.io/steady-cat-112112/rtmp-controller:latest",
            "ports": [
              {
                "containerPort": 80
              }
            ],
            "limits": {
              "memory": "100M",
              "cpu": "50m"
            }
          }
        ]
      }
    }
  }
}